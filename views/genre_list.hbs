<div class="jumbotron">
  <h1 class="display-4">Below are the List of Geners</h1>
  <p class="lead">You can view and manage the Genere!!!.</p>
  <hr class="my-4">
        <table class="table table-dark">
  <thead>
    <tr>
      
      <th scope="col">Genere Name</th>
      <th scope="col">Update Genere</th>
      <th scope="col">Delete Genere</th>
    </tr>
  </thead>
  <tbody>
        {{#each data}}
    <tr>
      
      <td><h5><a class = "text-success" href = {{this.url}}>{{this.name}} </a></h5></td>
      <td><a class = "btn btn-success" href = {{this.updateUrl}}>UpdateGenre</a></td>
      <td> <a class = " btn btn-danger" href = {{this.fullUrl}}>Delete Genere</a></td>
    </tr>
    <tr>
       {{/each}}
  </tbody>
</table>
</div>
